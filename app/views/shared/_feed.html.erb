<% if @feed_items.any? %>
  <ol class="microposts">
    <%# Render miroposts/_micropost.html.erb for each @feed_items %>
    <%= render @feed_items %>
  </ol>
  <%# explicit controller and action parameters %>
  <%= will_paginate @feed_items,
                    params: { controller: :static_pages, action: :home} %>
<% end %>
